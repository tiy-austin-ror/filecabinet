<h1>Homepage</h1>

<% if categories %>
  <% categories.each do |category| %>
    <p><%= link_to category.name, category %></p>
  <% end %>
<% end %>

<% if notes %>
  <% notes.each do |note| %>
    <p><%= link_to note.name, note %></p>
  <% end %>
<% end %>

<% if photos %>
  <% photos.each do |photo| %>
    <p><%= link_to photo.name, photo %></p>
  <% end %>
<% end %>
